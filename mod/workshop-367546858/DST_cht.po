msgstr "<number><rarity><c\nolour><item><plural>\n"

#: STRINGS.UI.TRADESCREEN.RECIPE_INGREDIENT_RARITY
msgid "Any <number> <rarity> Item<plural>"
msgstr "所有<number><rarity>\n的物品"

#: STRINGS.UI.TRADESCREEN.RECIPE_INGREDIENT_TAGS
msgid "<number> <rarity> <tags>Item<plural>"
msgstr "<number><rarity><t\nags>物品<plural>"

#: STRINGS.UI.TRADESCREEN.RECIPE_TITLE
msgid "Wanted: <rarity> Items"
msgstr "懸賞：<rarity>的物品"

